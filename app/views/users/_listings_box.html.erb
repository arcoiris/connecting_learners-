<div class="span3">
	<table class="table table-hover">	
<% @user.listings.each do |listing| %>
	<th><%= link_to "#{listing.title}", listing_path(listing.id) %> <% if current_user == @user %>    
		<%= link_to "Edit", edit_listing_path %> | <%= link_to "Delete",  listing_path, method: :delete, data: {confirm: 'Are you sure?'} %> <% end %></th></tr>
<% end %>
	</table>
</div>